# Example environment variables for backend
# Copy this file to `.env` and fill real values locally (DO NOT COMMIT `.env`).

PORT=8080
MONGO_URI=REPLACE_WITH_MONGODB_CONNECTION_STRING
SECERET_KEY=REPLACE_WITH_SECRET_KEY

# Gmail Configuration
GMAIL_USER=your_email@example.com
GMAIL_APP_PASSWORD=REPLACE_WITH_GMAIL_APP_PASSWORD

# Gemini API
GEMINI_API_KEY=REPLACE_WITH_GOOGLE_API_KEY

# Admin (optional)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=REPLACE_WITH_ADMIN_PASSWORD
